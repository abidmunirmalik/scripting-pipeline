// Scripted Pipeline - single-line comment

node('master') {
    stage('BUild') {
        buildFunction()
    }
    stage('Test') {
        testFunction()
    }
    stage('Deploy') {
        deployFunction()
    }
}
// Step functions
def buildFunction() {
    println("Building....")
}

def testFunction() {
    println("Testing....")
}

def deployFunction() {
    println("Deploying....")
}
